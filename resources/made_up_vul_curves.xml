    <vulnerabilityModel id="made_up_2012" assetCategory="buildings" lossCategory="structural">
        <description>Made up vulnerability curves</description>

        <vulnerabilityFunction id="dw1" dist="LN">
            <imls imt="0.2s gust at 10m height m/s">10.0 100.0</imls>
            <meanLRs>0 1</meanLRs>
            <covLRs>0 0</covLRs>
